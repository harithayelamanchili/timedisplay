import java.util.Calendar;
public class mytime{
	public static void main(String args[]){
		Calendar cal = Calendar.getInstance();

	timedisplay td=new timedisplay();
	td.time(cal.get(Calendar.HOUR_OF_DAY),cal.get(Calendar.MINUTE),cal.get(Calendar.SECOND)); 
	System.out.println(td.timeformat());
	System.out.println(td.timeregular());
	}
	}
